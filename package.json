{
  "name": "remark-code-blocks",
  "version": "1.0.0",
  "description": "Extract code blocks from an MDAST tree",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run test",
    "clean": "rimraf ./*.js coverage .nyc_output",
    "pretest": "npm run build",
    "lint": "xo --fix src/*.js",
    "tap": "tap -Rspec test.js",
    "build": "npm run lint && npm run babel",
    "babel": "babel src -d .",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc npm run tap"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "xo": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrzmmr/remark-code-blocks.git"
  },
  "keywords": [
    "remark",
    "util",
    "utility",
    "code",
    "blocks",
    "extract"
  ],
  "author": "mrzmmr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrzmmr/remark-code-blocks/issues"
  },
  "homepage": "https://github.com/mrzmmr/remark-code-blocks#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "nyc": "^13.0.1",
    "prettier": "^1.8.2",
    "remark": "^8.0.0",
    "rimraf": "^2.6.2",
    "tap": "^12.0.1",
    "unist-util-is": "^2.1.1",
    "xo": "^0.22.0"
  },
  "dependencies": {
    "unist-util-find-all-after": "^1.0.1"
  }
}
